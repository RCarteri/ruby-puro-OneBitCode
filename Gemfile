# Criar a pasta project e o arquivo Gemfile para poder instalar todas as gems de uma vez só
# Após criar a pasta rodar 
#   "gem install bundler"
#   "bundle" para instalar as gems do Gemfile

# Onde o ruby vai buscar as gems para instalar
source 'https://rubygems.org'

# Lista de Gems
gem 'os'
gem 'documentos_br'
gem 'nokogiri'